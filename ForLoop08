answer = str(input("Up or Down? ")).upper()
if answer == "UP":
    number = int(input("Pick a number above 1! "))
    number = number + 1
    for number in range (1,number,1):
        print(number)
elif answer == "DOWN":
    number = int(input("Pick a number below 20! "))
    number = number - 1
    for number in range (20,number,-1):
        print(number)
else:
    print("I don't understand!")
